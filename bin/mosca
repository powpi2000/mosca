#! /usr/bin/env node

var ascii = "" +
"       +++.+++:   ,+++    +++;   '+++    +++. \n" +
"      ++.+++.++   ++.++  ++,'+  `+',++  ++,++ \n" +
"      +`  +,  +: .+  .+  +;  +; '+  '+  +`  +` \n" +
"      +`  +.  +: ,+  `+  ++  +; '+  ;+  +   +. \n" +
"      +`  +.  +: ,+  `+   +'    '+      +   +. \n" +
"      +`  +.  +: ,+  `+   :+.   '+      +++++. \n" +
"      +`  +.  +: ,+  `+    ++   '+      +++++. \n" +
"      +`  +.  +: ,+  `+     ++  '+      +   +. \n" +
"      +`  +.  +: ,+  `+  +:  +: '+  ;+  +   +. \n" +
"      +`  +.  +: .+  .+  +;  +; '+  '+  +   +. \n" +
"      +`  +.  +:  ++;++  ++'++   ++'+'  +   +. \n" +
"      +`  +.  +:   +++    +++.   ,++'   +   +. ";

console.log(ascii);

require("../lib/cli")(process.argv, function(err) {
  if (err) {
    console.log(err);
    process.exit(1);
  }
  process.on("SIGTERM", function(cb){
    console.log("SIGTERM Catch!! broker server stop");
    process.exit(0);
  });
});
